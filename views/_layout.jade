  meta(property="og:url" content="http://45.76.55.231:3000/")
  meta(property="og:type" content="website")
  meta(property="og:image" content="http://45.76.55.231:3000/images/thumbnail.jpg")
  meta(property="og:description" content="전시회 아카이브")

  

append scripts
